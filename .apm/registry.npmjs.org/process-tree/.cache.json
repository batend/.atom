{\n\t\tconsole.log(JSON.stringify(children, null, 4));\n\t\tconsole.assert(children[0].ppid === pid);\n\t\tconsole.log(children[0].children[0].pid);\n\t\tconsole.log(children[0].name); // \"Name\" from wmic, \"comm\" from ps\n\t}\n});\n```\n","maintainers":[{"name":"1j01","email":"isaiahodhner@gmail.com"}],"time":{"modified":"2016-06-07T23:58:20.678Z","created":"2015-07-08T02:27:51.546Z","1.0.0":"2015-07-08T02:27:51.546Z","1.0.1":"2016-06-07T18:54:17.289Z","1.0.2":"2016-06-07T23:58:20.678Z"},"homepage":"https://github.com/1j01/process-tree#readme","keywords":["process","processes","tree","child","children","subprocess","ps","ps-tree","process-tree","top","htop","wmic","tasks","taskmgr","task","task-manager"],"repository":{"type":"git","url":"git+ssh://git@github.com/1j01/process-tree.git"},"bugs":{"url":"http://github.com/1j01/process-tree/issues"},"license":"MIT","readmeFilename":"README.md","_attachments":{},"_etag":"\"DT4WY3E26ESWC64J66ZX6FZQM\""}